{
  "name": "query-graphs-standalone-server",
  "version": "1.0.0",
  "private": true,
  "description": "Helping people see and understand queries.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tableau/query-graphs.git",
    "directory": "standalone-server"
  },
  "author": {
    "name": "Rick Cole",
    "email": "ricole@tableau.com"
  },
  "contributors": [
    {
      "name": "Luis Enciso",
      "email": "lenciso@tableau.com"
    },
    {
      "name": "Adrian Vogelsgesang",
      "email": "avogelsgesang@tableau.com"
    }
  ],
  "license": "MIT",
  "main": "./upload-server.js",
  "style": "app.css",
  "dependencies": {
    "body-parser": "^1.15.0",
    "browserify": "^16.0.0",
    "compression": "^1.7.1",
    "crypto": "0.0.3",
    "express": "^4.13.4",
    "fs": "0.0.2",
    "multer": "^1.1.0",
    "parcelify": "^2.2.1",
    "query-graphs": "file:../query-graphs",
    "recursive-readdir": "^2.2.1",
    "spin": "0.0.1",
    "uglify-js": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^2.2.0",
    "eslint-config-google": "^0.4.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "browserify app.js -d -o webroot/query-graphs.bundle.js -p [ parcelify -o webroot/query-graphs.bundle.css ]",
    "bundle-list": "browserify app.js --list",
    "prebundle-uglify": "npm run bundle",
    "bundle-uglify": "uglifyjs --compress --mangle -- webroot/query-graphs.bundle.js > webroot/query-graphs.min.js",
    "prebundle-tlv": "npm run bundle-uglify",
    "bundle-tlv": "tar -czf query-graphs-tlv-qt-$npm_package_version.tgz -C webroot query-graphs.tlv.html query-graphs.bundle.css query-graphs.min.js",
    "postinstall": "npm run bundle-uglify",
    "lint": "eslint ."
  }
}
